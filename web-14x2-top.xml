<Keyboard>
  <Name>SCK - Web 2 - Top</Name>
  <Width>100%</Width>
  <Height>23%</Height>
  <SymbolMargin>12</SymbolMargin>
  <BorderThickness>0,0,0,0</BorderThickness>
  <DockSize>Full</DockSize>
  <Position>Top</Position>
  <HorizontalOffset>0</HorizontalOffset>
  <VerticalOffset>0</VerticalOffset>
  <BackgroundColor>#ffffff</BackgroundColor>
  <BorderColor>#ffffff</BorderColor>
  <WindowState>Floating</WindowState>
  <HideFromKeyboardMenu>true</HideFromKeyboardMenu>
  <PersistNewState>false</PersistNewState>
  <Grid>
    <Rows>2</Rows>
    <Cols>14</Cols>
  </Grid>
  <KeyGroup 
    BackgroundColor="#18230F" 
    ForegroundColor="#ffffff" 
    ForegroundOpacity="0.75"
    Name="All" />
  <Content>
    <DynamicKey Row="0" Col="0">
      <Symbol>RestartIcon</Symbol>
      <Action>F5</Action>
    </DynamicKey>
    <DynamicKey Row="0" Col="1">
      <Symbol>BrowserSearchIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <Text>l</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="2">
      <Symbol>VolumeIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <Text>m</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="3">
      <Symbol>SearchTextIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <Text>c</Text>
      <Wait>50</Wait>
      <KeyUp>LCONTROL</KeyUp>
      <KeyDown>LCONTROL</KeyDown>
      <Text>t</Text>
      <KeyUp>LCONTROL</KeyUp>
      <Wait>100</Wait>
      <KeyDown>LCONTROL</KeyDown>
      <Text>v</Text>
      <KeyUp>LCONTROL</KeyUp>
      <Wait>50</Wait>
      <KeyDown>RETURN</KeyDown>
      <KeyUp>RETURN</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="4">
      <Symbol>ArrowPointingToTopBoundaryIcon</Symbol>
      <KeyDown>HOME</KeyDown>
      <KeyUp>HOME</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="5">
      <Symbol>ArrowPointingToBottomBoundaryIcon</Symbol>
      <KeyDown>END</KeyDown>
      <KeyUp>END</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="6">
      <Label>➕</Label>
      <KeyDown>LCONTROL</KeyDown>
      <Text>=</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="7">
      <Label>➖</Label>
      <KeyDown>LCONTROL</KeyDown>
      <Text>-</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="8">
      <Symbol>ArrowPointingToLeftIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <KeyDown>LSHIFT</KeyDown>
      <KeyDown>TAB</KeyDown>
      <KeyUp>TAB</KeyUp>
      <KeyUp>LSHIFT</KeyUp>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="9">
      <Symbol>ArrowPointingToRightIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <KeyDown>TAB</KeyDown>
      <KeyUp>TAB</KeyUp>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="10">
      <Symbol>BrowserFavoriteIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <Text>d</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="11">
      <Symbol>BrowserFullscreenIcon</Symbol>
      <Action> F11</Action>
    </DynamicKey>
    <DynamicKey Row="0" Col="12">
      <Symbol>BrowserNewTabIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <Text>t</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="13">
      <Symbol>BrowserCloseTabIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <Text>w</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="0">
      <Symbol>DownArrowKeyIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">web-14x2-bottom</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="1" Col="1">
      <Symbol>ArrowsAllDirectionsIcon</Symbol>    
      <Action>MouseMagneticCursor</Action>
    </DynamicKey>
    <DynamicKey Row="1" Col="2">
      <Symbol>LookToScrollActiveIcon</Symbol>
      <Action>LookToScrollActive</Action>
    </DynamicKey>
    <DynamicKey Row="1" Col="3">
      <Symbol>MouseLeftButtonClickIcon</Symbol>
      <Action>MouseMoveAndLeftClick</Action>
    </DynamicKey>
    <DynamicKey Row="1" Col="4">
      <Symbol>MouseLeftButtonDoubleClickIcon</Symbol>
      <Action>MouseMoveAndLeftDoubleClick</Action>
    </DynamicKey>
    <DynamicKey Row="1" Col="5">
      <Symbol>MouseMoveToPoint</Symbol>
      <Action>MouseMoveTo</Action>
    </DynamicKey>
    <DynamicKey Row="1" Col="6">
      <Symbol>KeyboardIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">keyboard-alphabet-14x4-top</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="1" Col="7">
      <Symbol>MouseNeutralIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">mouse-14x1-top</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="1" Col="8">
      <Symbol>MouseLeftClickDrag</Symbol>
      <Action>MouseDrag</Action>
    </DynamicKey>
    <DynamicKey Row="1" Col="9">
      <Symbol>MouseRightButtonClickIcon</Symbol>
      <Action>MouseMoveAndRightClick</Action>
    </DynamicKey>
    <DynamicKey Row="1" Col="10">
      <Symbol>BrowserBackIcon</Symbol>
      <KeyDown>LMENU</KeyDown>
      <KeyDown>LEFT</KeyDown>
      <KeyUp>LEFT</KeyUp>
      <KeyUp>LMENU</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="11">
      <Symbol>BrowserForwardIcon</Symbol>
      <KeyDown>LMENU</KeyDown>
      <KeyDown>RIGHT</KeyDown>
      <KeyUp>RIGHT</KeyUp>
      <KeyUp>LMENU</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="12">
      <Symbol>MoreIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">web-14x1-top</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="1" Col="13">
      <Symbol>NoIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">main-14x1-top</ChangeKeyboard>
    </DynamicKey>
  </Content>
</Keyboard>