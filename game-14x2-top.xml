<Keyboard>
  <Name>SCK - Web 2 - Top</Name>
  <Width>100%</Width>
  <Height>23%</Height>
  <SymbolMargin>12</SymbolMargin>
  <BorderThickness>0,0,0,0</BorderThickness>
  <DockSize>Full</DockSize>
  <Position>Top</Position>
  <HorizontalOffset>0</HorizontalOffset>
  <VerticalOffset>0</VerticalOffset>
  <BackgroundColor>#ffffff</BackgroundColor>
  <BorderColor>#ffffff</BorderColor>
  <WindowState>Floating</WindowState>
  <HideFromKeyboardMenu>true</HideFromKeyboardMenu>
  <PersistNewState>false</PersistNewState>
  <Grid>
    <Rows>2</Rows>
    <Cols>14</Cols>
  </Grid>
  <KeyGroup 
    BackgroundColor="#180F23" 
    ForegroundColor="#ffffff" 
    ForegroundOpacity="0.75"
    Name="All" />
  <Content>
    <DynamicKey Row="0" Col="0">
      <Symbol>ShiftKeyIcon</Symbol>
      <KeyToggle>LSHIFT</KeyToggle>
    </DynamicKey>
    <DynamicKey Row="0" Col="1">
      <Symbol>CtrlKeyIcon</Symbol>
      <KeyToggle>LCTRL</KeyToggle>
    </DynamicKey>
    <DynamicKey Row="0" Col="2">
      <Symbol>AltKeyIcon</Symbol>
      <KeyToggle>LMENU</KeyToggle>
    </DynamicKey>
    <DynamicKey Row="0" Col="3">
      <Symbol>WinKeyIcon</Symbol>
      <KeyToggle>LWin</KeyToggle>
    </DynamicKey>
    <DynamicKey Row="0" Col="4">
      <ShiftDownLabel>Z</ShiftDownLabel>
      <Label>z</Label>
      <Text>z</Text>
    </DynamicKey>
    <DynamicKey Row="0" Col="5">
      <Label>1</Label>
      <Text>1</Text>
    </DynamicKey>
    <DynamicKey Row="0" Col="6">
      <Label>2</Label>
      <Text>2</Text>
    </DynamicKey>
    <DynamicKey Row="0" Col="7">
      <Label>3</Label>
      <Text>3</Text>
    </DynamicKey>
    <DynamicKey Row="0" Col="8">
      <Label>4</Label>
      <Text>4</Text>
    </DynamicKey>
    <DynamicKey Row="0" Col="9">
      <ShiftDownLabel>A</ShiftDownLabel>
      <Label>a</Label>
      <KeyToggle>a</KeyToggle>
    </DynamicKey>
    <DynamicKey Row="0" Col="10">
      <ShiftDownLabel>S</ShiftDownLabel>
      <Label>s</Label>
      <KeyToggle>s</KeyToggle>
    </DynamicKey>
    <DynamicKey Row="0" Col="11">
      <ShiftDownLabel>D</ShiftDownLabel>
      <Label>d</Label>
      <KeyToggle>d</KeyToggle>
    </DynamicKey>
    <DynamicKey Row="0" Col="12">
      <ShiftDownLabel>X</ShiftDownLabel>
      <Label>x</Label>
      <Text>x</Text>
    </DynamicKey>
    <DynamicKey Row="0" Col="13">
      <ShiftDownLabel>R</ShiftDownLabel>
      <Label>r</Label>
      <Text>r</Text>
    </DynamicKey>
    <DynamicKey Row="1" Col="0">
      <Symbol>DownArrowKeyIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">game-14x1-bottom</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="1" Col="1">
      <Symbol>ArrowsAllDirectionsIcon</Symbol>    
      <Action>MouseMagneticCursor</Action>
    </DynamicKey>
    <DynamicKey Row="1" Col="2">
      <Symbol>EscapeKeyIcon</Symbol>
      <Action>Escape</Action>
    </DynamicKey>
    <DynamicKey Row="1" Col="3">
      <Label>‚ê£</Label>
      <Text>&#32;</Text>
    </DynamicKey>
    <DynamicKey Row="1" Col="4">
      <ShiftDownLabel>C</ShiftDownLabel>
      <Label>c</Label>  
      <Text>c</Text>
    </DynamicKey>
    <DynamicKey Row="1" Col="5">
      <ShiftDownLabel>I</ShiftDownLabel>
      <Label>i</Label>
      <Text>i</Text>
    </DynamicKey>
    <DynamicKey Row="1" Col="6">
      <Symbol>KeyboardIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">keyboard-alphabet-14x4-top</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="1" Col="7">
      <Symbol>MouseNeutralIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">mouse-14x1-top</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="1" Col="8">
      <ShiftDownLabel>F</ShiftDownLabel>
      <Label>f</Label>  
      <Text>f</Text>
    </DynamicKey>
    <DynamicKey Row="1" Col="9">
      <ShiftDownLabel>Q</ShiftDownLabel>
      <Label>q</Label>  
      <Text>q</Text>
    </DynamicKey>
    <DynamicKey Row="1" Col="10">
      <ShiftDownLabel>W</ShiftDownLabel>
      <Label>w</Label>  
      <KeyToggle>w</KeyToggle>
    </DynamicKey>
    <DynamicKey Row="1" Col="11">
      <ShiftDownLabel>E</ShiftDownLabel>
      <Label>e</Label>  
      <Text>e</Text>
    </DynamicKey>
    <DynamicKey Row="1" Col="12">
      <Symbol>MoreIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">game-14x2-top</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="1" Col="13">
      <Symbol>NoIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">main-14x1-top</ChangeKeyboard>
    </DynamicKey>
  </Content>
</Keyboard>