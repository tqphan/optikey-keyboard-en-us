<Keyboard>
  <Name>OptiKey-HorizontalBoard</Name>
  <Height>46%</Height>
  <SymbolMargin>12</SymbolMargin>
  <BorderThickness>0,0,0,0</BorderThickness>
  <DockSize>Full</DockSize>
  <Position>Top</Position>
  <Width>100%</Width>
  <HorizontalOffset>0</HorizontalOffset>
  <VerticalOffset>0</VerticalOffset>
  <BackgroundColor>#ffffff</BackgroundColor>
  <BorderColor>#ffffff</BorderColor>
  <WindowState>Floating</WindowState>
  <Grid>
    <Rows>4</Rows>
    <Cols>14</Cols>
  </Grid>
  <HideFromKeyboardMenu>true</HideFromKeyboardMenu>
  <PersistNewState>false</PersistNewState>
  <KeyGroup BackgroundColor="#333333" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Default" />
  <KeyGroup BackgroundColor="#2a2640" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Modifier" />
  <KeyGroup BackgroundColor="#2F4F4F" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Navigation" />
  <KeyGroup BackgroundColor="#333333" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Alphabet" />
  <KeyGroup BackgroundColor="#2c2c54" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Numeric" />
  <KeyGroup BackgroundColor="#c0392b" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Edit" />
  <KeyGroup BackgroundColor="#3F3F3F" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Punctuation" />
  <KeyGroup BackgroundColor="#10394c" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Pagination" />
  <KeyGroup BackgroundColor="#0c240c" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Exit" />
  <KeyGroup BackgroundColor="#0c240c" ForegroundColor="#ffffff" KeyDisabledBackground="gray"
    KeyDownBackground="blue" Name="Mirror" />
  <Content>
    <!-- Row 0 -->
    <DynamicKey Row="0" Col="0">
      <Symbol>ShiftKeyIcon</Symbol>
      <Action>LeftShift</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="1">
      <ShiftDownLabel>!</ShiftDownLabel>
      <Label>1</Label>
      <Text>1</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="2">
      <ShiftDownLabel>@</ShiftDownLabel>
      <Label>2</Label>
      <Text>2</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="3">
      <ShiftDownLabel>#</ShiftDownLabel>
      <Label>3</Label>
      <Text>3</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="4">
      <ShiftDownLabel>$</ShiftDownLabel>
      <Label>4</Label>
      <Text>4</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="5">
      <ShiftDownLabel>%</ShiftDownLabel>
      <Label>5</Label>
      <Text>5</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="6">
      <ShiftDownLabel>^</ShiftDownLabel>
      <Label>6</Label>
      <Text>6</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="7">
      <ShiftDownLabel>&amp;</ShiftDownLabel>
      <Label>7</Label>
      <Text>7</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="8">
      <ShiftDownLabel>*</ShiftDownLabel>
      <Label>8</Label>
      <Text>8</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="9">
      <ShiftDownLabel>(</ShiftDownLabel>
      <Label>9</Label>
      <Text>9</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="10">
      <ShiftDownLabel>)</ShiftDownLabel>
      <Label>0</Label>
      <Text>0</Text>
      <KeyGroup>Numeric</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="11">
      <ShiftDownLabel>_</ShiftDownLabel>
      <Label>-</Label>
      <Text>-</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="12">
      <ShiftDownLabel>+</ShiftDownLabel>
      <Label>=</Label>
      <Text>=</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="13">
      <Symbol>BackOneIcon</Symbol>
      <Action>BackOne</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <!-- Row 1 -->
    <DynamicKey Row="1" Col="0">
      <Symbol>WinKeyIcon</Symbol>
      <Action>LeftWin</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="1">
      <Label>!</Label>
      <Text>!</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="2">
      <Label>@</Label>
      <Text>@</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="3">
      <Label>#</Label>
      <Text>#</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="4">
      <Label>$</Label>
      <Text>$</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="5">
      <Label>%</Label>
      <Text>%</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="6">
      <Label>^</Label>
      <Text>^</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="7">
      <Label>&amp;</Label>
      <Text>&amp;</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="8">
      <Label>*</Label>
      <Text>*</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="9">
      <ShiftDownLabel>|</ShiftDownLabel>
      <Label>\</Label>
      <Text>\</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="10">
      <Label>⮝</Label>
      <KeyDown>UP</KeyDown>
      <KeyUp>UP</KeyUp>
      <KeyGroup>Navigation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="11">
      <Label>_</Label>
      <Text>_</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="12">
      <Label>+</Label>
      <Text>+</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="13">
      <Symbol>EnterIcon</Symbol>
      <Text>&#13;</Text>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <!-- Row 2 -->
    <DynamicKey Row="2" Col="0">
      <Symbol>CtrlKeyIcon</Symbol>
      <Action>LeftCtrl</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="1">
      <Symbol>AltKeyIcon</Symbol>
      <Action>LeftAlt</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="2">
      <Label>␣</Label>
      <Text>&#32;</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="3">
      <Label>(</Label>
      <Text>(</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="4">
      <Label>)</Label>
      <Text>)</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="5">
      <Label>[</Label>
      <Text>[</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="6">
      <Label>]</Label>
      <Text>]</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="7">
      <Label>{</Label>
      <Text>{</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="8">
      <Label>}</Label>
      <Text>}</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="9">
      <Label>⮜</Label>
      <KeyDown>LEFT</KeyDown>
      <KeyUp>LEFT</KeyUp>
      <KeyGroup>Navigation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="10">
      <Label>⮟</Label>
      <KeyDown>DOWN</KeyDown>
      <KeyUp>DOWN</KeyUp>
      <KeyGroup>Navigation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="11">
      <Label>⮞</Label>
      <KeyDown>RIGHT</KeyDown>
      <KeyUp>RIGHT</KeyUp>
      <KeyGroup>Navigation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="12">
      <Label>~</Label>
      <Text>~</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="13">
      <ShiftDownLabel>~</ShiftDownLabel>
      <Label>`</Label>
      <Text>`</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <!-- Row 3 -->
    <DynamicKey Row="3" Col="0">
      <Symbol>DownArrowKeyIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">keyboard-numeric-14x4-bottom</ChangeKeyboard>
      <KeyGroup>Mirror</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="1">
      <Symbol>AlphaIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">keyboard-alphabet-14x4-top</ChangeKeyboard>
      <KeyGroup>Pagination</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="2">
      <Symbol>ClearIcon</Symbol>
      <Action>ClearScratchpad</Action>
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
    <SuggestionRow Row="3" Col="3" Width="8">
      <KeyGroup>Suggestion</KeyGroup>
    </SuggestionRow>
    <DynamicKey Row="3" Col="11">
      <Symbol>Sleep</Symbol>
      <Action>Sleep</Action>
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="12">
      <Symbol>PhysicalKeysIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">keyboard-function-14x4-top</ChangeKeyboard>
      <KeyGroup>Pagination</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="13">
      <Symbol>NoIcon</Symbol>
      <Action>BackFromKeyboard</Action>
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
  </Content>
</Keyboard>