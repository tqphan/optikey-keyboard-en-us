      <Text>'</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey row="1" Col="13">
      <Symbol>BackspaceKeyIcon</Symbol>
      <Action>BackOne</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <!-- Row 2 -->
