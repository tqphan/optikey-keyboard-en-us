<Keyboard>
  <Name>OptiKey-HorizontalBoard</Name>
  <Height>46%</Height>
  <SymbolMargin>12</SymbolMargin>
  <BorderThickness>0,0,0,0</BorderThickness>
  <DockSize>Full</DockSize>
  <Position>Bottom</Position>
  <Width>100%</Width>
  <HorizontalOffset>0</HorizontalOffset>
  <VerticalOffset>0</VerticalOffset>
  <BackgroundColor>#ffffff</BackgroundColor>
  <BorderColor>#ffffff</BorderColor>
  <WindowState>Floating</WindowState>
  <Grid>
    <Rows>4</Rows>
    <Cols>14</Cols>
  </Grid>
  <HideFromKeyboardMenu>true</HideFromKeyboardMenu>
  <PersistNewState>false</PersistNewState>
  <!-- <KeyGroup Name="All" BackgroundColor="#333333" KeyDisabledBackground="gray"
  KeyDownBackground="blue" /> -->
  <!-- <KeyGroup Name="Modifier" BackgroundColor="#570211"/>
  <KeyGroup Name="Navigation" BackgroundColor="#004540"/>
  <KeyGroup Name="Alphabet" BackgroundColor="#434343" ForegroundColor="#ffffff"/>
  <KeyGroup Name="Numeric" BackgroundColor="#2a2640" ForegroundColor="#ffffff"/>
  <KeyGroup Name="Edit" BackgroundColor="#2a2640"/>
  <KeyGroup Name="Punctuation" BackgroundColor="#7e3110"/>
  <KeyGroup Name="Pagination" BackgroundColor="#10394c"/>
  <KeyGroup Name="Exit" BackgroundColor="#0c240c"/>
  <KeyGroup Name="Mirror" BackgroundColor="#0c240c"/> -->
  <Content>
    <!-- Row 0 -->
    <DynamicKey row="0" Col="0">
      <Symbol>UpArrowKeyIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">keyboard-alphabet-14x4-top</ChangeKeyboard>
      <KeyGroup>Mirror</KeyGroup>
    </DynamicKey>
    <DynamicKey row="0" Col="1">
      <Symbol>PhysicalKeysIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">keyboard-function-14x4-bottom</ChangeKeyboard>
      <KeyGroup>Pagination</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="2">
      <Symbol>EraserIcon</Symbol>
      <Action>ClearScratchpad</Action>
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
    <SuggestionRow Row="0" Col="3" Width="8">
      <KeyGroup>Suggestion</KeyGroup>
    </SuggestionRow>
    <DynamicKey Row="0" Col="11">
      <Symbol>ConversationIcon</Symbol>
      <KeyDown>LWIN</KeyDown>
      <Text>c</Text>
      <KeyUp>LWIN</KeyUp>
      <Action>ClearScratchpad</Action>
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
    <DynamicKey row="0" Col="12">
      <Symbol>NumericAndSymbolsIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">keyboard-numeric-14x4-bottom</ChangeKeyboard>
      <KeyGroup>Pagination</KeyGroup>
    </DynamicKey>
    <DynamicKey row="0" Col="13">
      <Symbol>NoIcon</Symbol>
      <Action>BackFromKeyboard</Action>
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
    <!-- Row 1 -->
    <DynamicKey row="1" Col="0">
      <Symbol>ShiftKeyIcon</Symbol>
      <Action>LeftShift</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="1">
      <Symbol>TabKeyIcon</Symbol>
      <Text>&#9;</Text>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey row="1" Col="2">
      <ShiftDownLabel>Q</ShiftDownLabel>
      <Label>q</Label>
      <Text>q</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey row="1" Col="3">
      <ShiftDownLabel>W</ShiftDownLabel>
      <Label>w</Label>
      <Text>w</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey row="1" Col="4">
      <ShiftDownLabel>E</ShiftDownLabel>
      <Label>e</Label>
      <Text>e</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey row="1" Col="5">
      <ShiftDownLabel>R</ShiftDownLabel>
      <Label>r</Label>
      <Text>r</Text>
      <KeyGroup>Alphabet</KeyGroup>
