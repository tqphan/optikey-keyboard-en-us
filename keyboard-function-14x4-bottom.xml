<Keyboard>
  <Name>OptiKey-HorizontalBoard</Name>
  <Height>46%</Height>
  <SymbolMargin>12</SymbolMargin>
  <BorderThickness>0,0,0,0</BorderThickness>
  <DockSize>Full</DockSize>
  <Position>Bottom</Position>
  <Width>100%</Width>
  <HorizontalOffset>0</HorizontalOffset>
  <VerticalOffset>0</VerticalOffset>
  <BackgroundColor>#ffffff</BackgroundColor>
  <BorderColor>#ffffff</BorderColor>
  <WindowState>Floating</WindowState>
  <Grid>
    <Rows>4</Rows>
    <Cols>14</Cols>
  </Grid>
  <HideFromKeyboardMenu>true</HideFromKeyboardMenu>
  <PersistNewState>false</PersistNewState>
  <!-- <KeyGroup Name="All" BackgroundColor="#333333" KeyDisabledBackground="gray"
  KeyDownBackground="blue" /> -->
  <KeyGroup Name="Modifier" BackgroundColor="#2a2640" ForegroundColor="#ffffff"
    KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Function" BackgroundColor="#2a2640" ForegroundColor="#ffffff"
    KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Navigation" BackgroundColor="DarkSlateGray" KeyDisabledBackground="gray"
    KeyDownBackground="blue" />
  <KeyGroup Name="Alphabet" BackgroundColor="#333333" ForegroundColor="#ffffff"
    KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Numeric" BackgroundColor="#2a2640" ForegroundColor="#ffffff"
    KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Edit" BackgroundColor="#2a2640" ForegroundColor="#ffffff"
    KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Punctuation" BackgroundColor="#3F3F3F" ForegroundColor="#ffffff"
    KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Pagination" BackgroundColor="#10394c" ForegroundColor="#ffffff"
    KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Exit" BackgroundColor="#0c240c" ForegroundColor="#ffffff"
    KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Mirror" BackgroundColor="#0c240c" ForegroundColor="#ffffff"
    KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <Content>
    <!-- Row 0 -->
    <DynamicKey Row="0" Col="0">
      <Symbol>UpArrowKeyIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">keyboard-function-14x4-top</ChangeKeyboard>
      <KeyGroup>Mirror</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="1">
      <Symbol>NumericAndSymbolsIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">keyboard-numeric-14x4-bottom</ChangeKeyboard>
      <KeyGroup>Pagination</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="2">
      <Symbol>ClearIcon</Symbol>
      <Action>ClearScratchpad</Action>
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
    <SuggestionRow Row="0" Col="3" Width="8">
      <KeyGroup>Suggestion</KeyGroup>
    </SuggestionRow>
    <DynamicKey Row="0" Col="11">
      <Symbol>Sleep</Symbol>
      <Action>Sleep</Action>
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="12">
      <Symbol>AlphaIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">keyboard-alphabet-14x4-bottom</ChangeKeyboard>
      <KeyGroup>Pagination</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="13">
      <Symbol>NoIcon</Symbol>
      <Action>BackFromKeyboard</Action>
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
    <!-- Row 1 -->
    <DynamicKey Row="1" Col="0">
      <Symbol>ShiftKeyIcon</Symbol>
      <Action>LeftShift</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="1">
      <Label>F1</Label>
      <Action>F1</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="2">
      <Label>F2</Label>
      <Action>F2</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="3">
      <Label>F3</Label>
      <Action>F3</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="4">
      <Label>F4</Label>
      <Action>F4</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="5">
      <Label>F5</Label>
      <Action>F5</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="6">
      <Label>F6</Label>
      <Action>F6</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="7">
      <Label>F7</Label>
      <Action>F7</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="8">
      <Label>F8</Label>
      <Action>F8</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="9">
      <Label>F9</Label>
      <Action>F9</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="10">
      <Label>F10</Label>
      <Action>F10</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="11">
      <Label>F11</Label>
      <Action>F11</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="12">
      <Label>F12</Label>
      <Action>F12</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="13">
      <Symbol>BackOneIcon</Symbol>
      <Action>BackOne</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <!-- Row 2 -->
    <DynamicKey Row="2" Col="0">
      <Symbol>WinKeyIcon</Symbol>
      <Action>LeftWin</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="1">
      <Label>Esc</Label>
      <KeyDown>ESCAPE</KeyDown>
      <KeyUp>ESCAPE</KeyUp>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="2">
      <Label>Caps</Label>
      <KeyDown>CAPITAL</KeyDown>
      <KeyUp>CAPITAL</KeyUp>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="3">
      <Label>Home</Label>
      <Action>Home</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="4">
      <Label>PgUp</Label>
      <Action>PgUp</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="5">
      <Label>Ins</Label>
      <Action>Insert</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="6">
      <KeyGroup>Blank</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="7">
      <KeyGroup>Blank</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="8">
      <KeyGroup>Blank</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="9">
      <Label>Del</Label>
      <KeyDown>DELETE</KeyDown>
      <KeyUp>DELETE</KeyUp>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="10">
      <Label>⮝</Label>
      <KeyDown>UP</KeyDown>
      <KeyUp>UP</KeyUp>
      <KeyGroup>Navigation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="11">
      <Symbol>WinKeyIcon</Symbol>
      <Action>RightWin</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="12">
      <Symbol>ShiftKeyIcon</Symbol>
      <Action>RightShift</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="13">
      <Symbol>EnterIcon</Symbol>
      <Text>&#13;</Text>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <!-- Row 3 -->
    <DynamicKey Row="3" Col="0">
      <Symbol>CtrlKeyIcon</Symbol>
      <Action>LeftCtrl</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="1">
      <Symbol>AltKeyIcon</Symbol>
      <KeyToggle>LMENU</KeyToggle>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey row="3" Col="2">
      <Label>␣</Label>
      <Text>&#32;</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="3">
      <Label>End</Label>
      <Action>End</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="4">
      <Label>PgDn</Label>
      <Action>PgDn</Action>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="5">
      <Label>p|b</Label>
      <KeyDown>PAUSE</KeyDown>
      <KeyUp>PAUSE</KeyUp>
      <KeyGroup>Function</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="6">
      <KeyGroup>Blank</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="7">
      <KeyGroup>Blank</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="8">
      <KeyGroup>Blank</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="9">
      <Label>⮜</Label>
      <KeyDown>LEFT</KeyDown>
      <KeyUp>LEFT</KeyUp>
      <KeyGroup>Navigation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="10">
      <Label>⮟</Label>
      <KeyDown>DOWN</KeyDown>
      <KeyUp>DOWN</KeyUp>
      <KeyGroup>Navigation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="11">
      <Label>⮞</Label>
      <KeyDown>RIGHT</KeyDown>
      <KeyUp>RIGHT</KeyUp>
      <KeyGroup>Navigation</KeyGroup>
    </DynamicKey>
    <DynamicKey row="3" Col="12">
      <Symbol>AltKeyIcon</Symbol>
      <Action>RightAlt</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey row="3" Col="13">
      <Symbol>CtrlKeyIcon</Symbol>
      <Action>RightCtrl</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
  </Content>
</Keyboard>